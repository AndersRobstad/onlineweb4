{
  "presets" : [
    ["env", {
      "modules": false
    }],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-runtime", { "polyfill": false }],
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
