{
  "presets" : [
    ["env", {
      "modules": "commonjs" // set to false when upgrading to webpack >=2
    }],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
